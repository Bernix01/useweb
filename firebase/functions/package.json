{
  "name": "useweb-functions",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm:build:watch' 'npm:serve'",
    "build": "node scripts/buildFunctions/run.js",
    "build:watch": "node scripts/buildFunctions/run.js --watch",
    "serve": "firebase emulators:start --only functions",
    "deploy": "firebase deploy --only functions"
  },
  "engines": {
    "node": "14"
  },
  "main": "build/index.js",
  "dependencies": {
    "command-line-args": "^5.2.0",
    "concurrently": "^6.4.0",
    "esbuild": "^0.14.2",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1"
  },
  "devDependencies": {
    "typescript": "^3.8.0",
    "firebase-functions-test": "^0.2.0"
  }
}
